{
  "name": "willg.dev",
  "private": true,
  "description": "Will Golledge portfolio site",
  "version": "1.0.0",
  "author": "Will Golledge <hi@willg.dev>",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write src/**/*.{js,jsx}",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "type-check": "tsc --noEmit",
    "lint": "eslint --ignore-path .gitignore --ext ts --ext tsx --ext js --ext jsx src",
    "lint:fix": "npm run lint --fix"
  },
  "dependencies": {
    "babel-plugin-styled-components": "1.10.6",
    "gatsby": "^2.23.14",
    "gatsby-image": "2.2.17",
    "gatsby-plugin-manifest": "2.2.14",
    "gatsby-plugin-offline": "2.2.10",
    "gatsby-plugin-prefetch-google-fonts": "1.4.3",
    "gatsby-plugin-react-helmet": "3.1.6",
    "gatsby-plugin-sharp": "2.2.20",
    "gatsby-plugin-styled-components": "3.1.4",
    "gatsby-plugin-typescript": "2.1.6",
    "gatsby-source-filesystem": "2.1.21",
    "gatsby-transformer-json": "2.2.7",
    "gatsby-transformer-sharp": "2.2.13",
    "gsap": "2.1.3",
    "pixi.js": "5.1.2",
    "prop-types": "15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "5.2.1",
    "styled-components": "4.3.2",
    "three": "0.106.2",
    "unique-random-array": "2.0.0"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.10.4",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@types/gsap": "1.20.2",
    "@types/jest": "^24.9.1",
    "@types/node": "12.7.4",
    "@types/react": "16.9.13",
    "@types/react-dom": "16.9.4",
    "@types/react-helmet": "5.0.9",
    "@types/styled-components": "4.1.18",
    "@typescript-eslint/eslint-plugin": "1.13.0",
    "@typescript-eslint/parser": "1.13.0",
    "babel-jest": "^24.9.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.3.0",
    "eslint-plugin-jest": "^23.17.1",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.3",
    "husky": "2.7.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^24.9.0",
    "jest-preset-gatsby": "^1.0.65",
    "lint-staged": "8.2.1",
    "prettier": "1.18.2",
    "react-test-renderer": "^16.13.1",
    "typescript": "3.6.2"
  },
  "keywords": [
    "react",
    "gatsby",
    "leon-sans"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run type-check && lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "npm run lint:fix",
      "git add"
    ],
    "{*.{json,md}}": [
      "prettier --write",
      "git add"
    ]
  }
}
